{
  "name": "Teacher",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start-reset": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "start-iphone": "react-native run-ios --simulator='iPhone 8' --project-path ../../ --scheme Teacher",
    "start-iphone-x": "react-native run-ios --simulator='iPhone X' --project-path ../../ --scheme Teacher",
    "start-ipad": "react-native run-ios --simulator='iPad Pro (10.5-inch)' --project-path ../../ --scheme Teacher",
    "build": "yarn && pod install",
    "build-update": "yarn && pod install --repo-update",
    "clean": "rm -rf \"$HOME/Library/Developer/Xcode/DerivedData/\" && rm -rf './node_modules' && rm -rf '../../build' && pod cache clean --all",
    "test": "jest",
    "lint": "eslint *.js __tests__/*.js src test flow",
    "lint:fix": "eslint --fix *.js __tests__/*.js src test",
    "lint:watch": "esw -w",
    "flow": "flow check",
    "extract-strings": "format-message extract -g underscored_crc32 \"src/**/*.js\" index.ios.js -o i18n/locales/en.json",
    "danger": "danger",
    "packager:kill": "react-native-kill-packager",
    "postinstall": "./rn-mods.sh",
    "prepush": "yarn test --coverage && yarn lint:fix && yarn flow",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "optionalDependencies": {
    "fsevents": "^1.2.4"
  },
  "dependencies": {
    "apollo-client-preset": "^1.0.8",
    "create-react-class": "15.6.2",
    "flux-standard-action": "^1.2.0",
    "format-message": "^6.0.4",
    "global": "^4.3.2",
    "graphql": "^0.13.1",
    "graphql-tag": "^2.7.3",
    "hoist-non-react-statics": "^1.2.0",
    "html-entities": "^1.2.1",
    "hyntax": "^1.0.4",
    "knuth-shuffle-seeded": "^1.0.6",
    "lodash": "^4.17.14",
    "md5": "^2.2.1",
    "prop-types": "^15.6.0",
    "querystring": "^0.2.0",
    "react": "16.0.0",
    "react-apollo": "^2.0.4",
    "react-native": "0.51.0",
    "react-native-audio": "^3.5.1",
    "react-native-button": "^2.1.0",
    "react-native-camera": "^0.10.0",
    "react-native-counter": "Kerumen/react-native-counter",
    "react-native-device-info": "^0.10.1",
    "react-native-document-picker": "Elyx0/react-native-document-picker",
    "react-native-fs": "^2.3.2",
    "react-native-hyperlink": "obipawan/react-native-hyperlink",
    "react-native-image-picker": "^0.26.3",
    "react-native-interactable": "derrh/react-native-interactable",
    "react-native-keyboard-aware-scroll-view": "^0.7.4",
    "react-native-keyboard-spacer": "n8armstrong/react-native-keyboard-spacer",
    "react-native-linear-gradient": "^2.4.0",
    "react-native-on-layout": "^2.0.2",
    "react-native-progress": "^3.5.0",
    "react-native-search-bar": "^3.4.0",
    "react-native-sfsafariviewcontroller": "skycocker/react-native-sfsafariviewcontroller",
    "react-native-sound": "^0.10.4",
    "react-native-status-bar-size": "^0.3.3",
    "react-native-test-utils": "^1.0.6",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-actions": "^1.2.2",
    "striptags": "^3.0.1",
    "url-parse": "^1.4.3",
    "uuid": "^3.1.0",
    "valid-url": "1.0.9",
    "snyk": "^1.124.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-jest": "^20.0.3",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-format-message": "^6.0.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-native": "2.1.0",
    "clarify": "^2.1.0",
    "danger": "^3.7.18",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-format-message": "^6.0.4",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-standard": "^2.0.1",
    "eslint-watch": "^3.0.0",
    "flow-bin": "^0.63.0",
    "format-message-cli": "^6.0.4",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "jest-fetch-mock": "^1.4.1",
    "prompt": "1.0",
    "react-dom": "^16.1.1",
    "react-native-kill-packager": "^1.0.0",
    "react-test-renderer": "^16.1.1",
    "redux-freeze": "^0.1.5",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.2.3",
    "timezone-mock": "^0.0.4",
    "trace": "^3.1.0",
    "yargs": "^12.0.2"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|my-project|react-native-button)"
    ],
    "setupFiles": [
      "<rootDir>/test/scripts/jestSetup.js"
    ],
    "moduleNameMapper": {
      "^.+\\.html$": "<rootDir>/test/scripts/html-mock"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverage": false,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      ".android.js",
      "flow/*.js",
      "/src/modules/developer-menu",
      "dangerfile.js",
      "/src/canvas-api/apis",
      "/test/scripts"
    ],
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "json",
      "json-summary",
      "lcov",
      "text-summary",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 80,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "snyk": true
}

{
  "name": "Teacher",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "start-reset": "react-native start --reset-cache",
    "start-iphone": "react-native run-ios --simulator='iPhone 8' --project-path ../../ --scheme Teacher",
    "start-iphone-x": "react-native run-ios --simulator='iPhone X' --project-path ../../ --scheme Teacher",
    "start-ipad": "react-native run-ios --simulator='iPad Pro (10.5-inch)' --project-path ../../ --scheme Teacher",
    "build": "yarn && pod install",
    "build-update": "yarn && pod install --repo-update",
    "clean": "rm -rf \"$HOME/Library/Developer/Xcode/DerivedData/\" && rm -rf './node_modules' && rm -rf '../../build' && pod cache clean --all",
    "test": "jest",
    "test:ci": "set -o pipefail; mkdir -p ../../tmp; jest --silent --coverage --color 2>&1 | tee ../../tmp/yarn-test-results.txt",
    "lint": "eslint *.js **/__tests__/*.js src test flow",
    "lint:fix": "eslint --fix *.js **/__tests__/*.js src test flow",
    "lint:watch": "esw -w",
    "lint:ci": "set -o pipefail; mkdir -p ../../tmp; yarn --silent lint --color | tee ../../tmp/yarn-lint-results.txt",
    "flow": "echo 'flow check is disabled'",
    "extract-strings": "format-message extract -g underscored_crc32 \"src/**/*.js\" index.ios.js -o i18n/locales/en.json",
    "danger": "danger",
    "packager:kill": "react-native-kill-packager",
    "postinstall": "./rn-mods.sh",
    "prepush": "yarn test --coverage && yarn lint:fix && yarn flow"
  },
  "optionalDependencies": {
    "fsevents": "^1.2.4"
  },
  "dependencies": {
    "@react-native-community/art": "^1.2.0",
    "@react-native-community/async-storage": "^1.6.1",
    "apollo-client-preset": "^1.0.8",
    "apollo-link-error": "^1.1.12",
    "create-react-class": "15.6.2",
    "flux-standard-action": "^1.2.0",
    "format-message": "^6.2.3",
    "format-message-generate-id": "^6.2.3",
    "graphql": "^0.13.1",
    "graphql-tag": "^2.7.3",
    "hoist-non-react-statics": "^3.3.0",
    "knuth-shuffle-seeded": "^1.0.6",
    "lodash": "^4.17.14",
    "md5": "^2.2.1",
    "prop-types": "^15.6.0",
    "react": "16.11.0",
    "react-apollo": "^2.0.4",
    "react-native": "0.62.2",
    "react-native-audio": "^3.5.1",
    "react-native-button": "^2.1.0",
    "react-native-camera": "^3.23.1",
    "react-native-counter": "Kerumen/react-native-counter",
    "react-native-document-picker": "Elyx0/react-native-document-picker",
    "react-native-fs": "^2.3.2",
    "react-native-hyperlink": "obipawan/react-native-hyperlink",
    "react-native-image-picker": "^2.3.1",
    "react-native-interactable": "derrh/react-native-interactable",
    "react-native-keyboard-aware-scroll-view": "^0.8.0",
    "react-native-keyboard-spacer": "n8armstrong/react-native-keyboard-spacer",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-on-layout": "^2.0.2",
    "react-native-progress": "^4.1.2",
    "react-native-search-bar": "^3.4.3",
    "react-native-sound": "^0.11.0",
    "react-native-test-utils": "^1.0.6",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-actions": "^1.2.2",
    "striptags": "^3.0.1",
    "url-parse": "^1.4.7",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.4.0",
    "clarify": "^2.1.0",
    "danger": "^3.7.18",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^6.2.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-format-message": "~6.0.4",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-standard": "^2.0.1",
    "eslint-watch": "^6.0.1",
    "flow-bin": "^0.113.0",
    "format-message-cli": "^6.2.3",
    "jest": "^25.4.0",
    "jest-cli": "^25.4.0",
    "jest-fetch-mock": "^3.0.3",
    "metro-react-native-babel-preset": "^0.58.0",
    "react-dom": "^16.11.0",
    "react-native-kill-packager": "^1.0.0",
    "react-test-renderer": "16.11.0",
    "redux-freeze": "^0.1.5",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.2.3",
    "timezone-mock": "^0.0.4",
    "trace": "^3.1.0"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|my-project|react-native-button|@react-native-community)"
    ],
    "setupFiles": [
      "<rootDir>/test/scripts/jestSetup.js"
    ],
    "moduleNameMapper": {
      "^.+\\.html$": "<rootDir>/test/scripts/html-mock"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverage": false,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      ".android.js",
      "flow/*.js",
      "/src/modules/developer-menu",
      "dangerfile.js",
      "/src/canvas-api/apis",
      "/test/scripts"
    ],
    "coverageDirectory": "../../scripts/coverage/react-native",
    "coverageReporters": [
      "json-summary",
      "text-summary",
      "html",
      "json"
    ]
  }
}
